<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">


    <changeSet id="migration-original-logins" author="frolov" context="dev or prod">
        <sql>
            insert into original_login (user_id, name, source) select id, name, source from user;
        </sql>
    </changeSet>

    <changeSet id="update-user-is-active" author="frolov" context="prod">
        <sql>
            update user SET is_active = 1;
        </sql>
    </changeSet>

</databaseChangeLog>