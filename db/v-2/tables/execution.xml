<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <changeSet id="add-execCmd-and-batchSizeForAnalyzer-columns" author="kgevorkyan" context="dev or prod">
        <addColumn tableName="execution">
            <column name="execCmd" type="varchar(255)" defaultValue="NULL"/>
        </addColumn>

        <addColumn tableName="execution">
            <column name="batchSizeForAnalyzer" type="varchar(255)" defaultValue="NULL"/>
        </addColumn>
    </changeSet>

</databaseChangeLog>